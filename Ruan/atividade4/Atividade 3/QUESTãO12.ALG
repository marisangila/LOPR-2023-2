Algoritmo "Questão12"

Var
   nA: real
   nB: real
   nC: real
Inicio
       escreval("Escolha um valor para A")
       leia(nA)
       escreval("Escolha um valor para B")
       leia(nB)
       nC <- nA
       nA <- nB
       nB <- nC
       escreva("O valor A é" , nA , " e B é " , nb)
Fimalgoritmo